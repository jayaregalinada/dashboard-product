{
    "name": "jag/dashboard-product",
    "description": "Add Products to your dashboard",
    "type": "library",
    "keywords": ["jag", "jayaregalinada", "dashboard", "philippines", "admin", "panel", "control", "product", "ecommerce", "e-commerce", "commerce", "sms", "chikka"],
    "license": "MIT",
    "authors": [
        {
            "name": "jayaregalinada",
            "email": "jayaregalinada@gmail.com",
            "homepage": "http://jayaregalinada.github.io/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "jag/dashboard": "dev-master",
        "intervention/image": "^2.4",
        "intervention/imagecache": "^2.3",
        "jag/chikka": "^0.1.4",
        "illuminate/support": "~5.5",
        "cviebrock/eloquent-sluggable": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Jag\\Dashboard\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        },
        "laravel": {
            "providers": [
                "Jag\\Dashboard\\Providers\\ProductServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
